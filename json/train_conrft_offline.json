{
    "output_dir": "outputs/train_conrft_offline_test",
    "job_name": "conrft_offline_test",
    "resume": false,
    "seed": 1000,
    "num_workers": 4,
    "batch_size": 256,
    "steps": 2,
    "log_freq": 10,
    "save_checkpoint": true,
    "save_freq": 500,
    "wandb": {
        "enable": true,
        "project": "conrft-offline-test",
        "disable_artifact": true
    },
    "dataset": {
        "repo_id": "lilkm/panda_pick_octo_resized",
        "use_imagenet_stats": true
    },
    "policy": {
        "type": "conrft",
        "push_to_hub": false,
        "n_obs_steps": 1,
        "n_action_steps": 4,
        "chunk_size": 10,
        "normalization_mapping": {
            "VISUAL": "IDENTITY",
            "STATE": "MIN_MAX",
            "ACTION": "MIN_MAX"
        },
        "input_features": {
            "observation.images.front": {
                "type": "VISUAL",
                "shape": [3, 256, 256]
            },
            "observation.images.wrist": {
                "type": "VISUAL", 
                "shape": [3, 128, 128]
            },
            "observation.state": {
                "type": "STATE",
                "shape": [18]
            }
        },
        "output_features": {
            "action": {
                "type": "ACTION",
                "shape": [4]
            }
        },
        "dataset_stats": {
