        "episode_length": 100,
        "use_viewer": true,
        "use_gamepad": true,
        "device": "cpu",
        "wrapper": {
            "gripper_penalty": 0.0,
            "display_cameras": false,
            "add_joint_velocity_to_observation": false,
            "add_ee_pose_to_observation": false,
            "crop_params_dict": {
                "observation.images.front": [0, 0, 256, 256],
                "observation.images.wrist": [0, 0, 128, 128]
