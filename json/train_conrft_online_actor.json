{
    "output_dir": null,
    "job_name": "conrft_online_actor",
    "resume": false,
    "seed": 1000,
    "num_workers": 4,
    "batch_size": 128,
    "steps": 100000,
    "log_freq": 10,
    "save_checkpoint": false,
    "wandb": {
        "enable": true,
        "project": "conrft-online-test",
        "disable_artifact": true
    },
    "dataset": {
        "repo_id": "lilkm/panda_pick_cube_conrft_with_action_embeddings",
        "use_imagenet_stats": true
    },
    "policy": {
        "type": "conrft",
        "push_to_hub": false,
        "n_obs_steps": 1,
        "n_action_steps": 4,
        "chunk_size": 10,
        "normalization_mapping": {
            "VISUAL": "IDENTITY",
            "STATE": "MIN_MAX",
            "ACTION": "MIN_MAX"
        },
        "input_features": {
